{
  "compilerOptions": {
    "target": "ES2020",
    "allowJs": true,
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "noImplicitAny": true,
    "noImplicitThis": true,
    "alwaysStrict": true,
    "strictNullChecks": true,
    "strictPropertyInitialization": true,
    "skipLibCheck": true,
    "noFallthroughCasesInSwitch": true,
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "noImplicitReturns": true,
    "noEmitOnError": false,
    "noEmit": true,
    "inlineSourceMap": true,
    "inlineSources": true,
    "baseUrl": ".",
    "module": "es2020",
    "experimentalDecorators": true,
    "lib": ["ES2017", "ES2019.Object", "ES2020", "DOM", "DOM.Iterable", "ES2021.String"],
    "paths": {
      "*": ["types/*"],
      "mow-registry": ["app"],
      "mow-registry/*": ["app/*"],
      "fetch": [
        "node_modules/ember-fetch"
      ]
    }
  },
  "include": ["app/**/*", "tests/**/*", "types/**/*", "test-support/**/*"]
}
